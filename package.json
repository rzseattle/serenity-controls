{
    "name": "serenity-controls",
    "version": "2.0.36",
    "description": "",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "scripts": {
        "watch": "npm-run-all -p 'build:* -- --watch'",
        "dev": "npm-run-all -p storybook 'build:modules -- --watch'",
        "build": "npm-run-all 'build:*'",
        "build:prettier": "prettier --write src",
        "build:modules": "tsm \"src/**/*.module.sass\" ",
        "build:typescript": "tsc --project ./ --outDir ./lib ",
        "build:sass": "cpx 'src/**/*.sass' lib",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook -ci --quiet -c ./docs/stories/.storybook",
        "prepare": "npm-run-all build:modules build:sass build:typescript",
        "husky-run": "echo 1",
        "lint": "eslint src/**",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --watchAll=false --coverage"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn run build"
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "prettier": {
        "useTabs": false,
        "tabWidth": 4,
        "printWidth": 120,
        "arrowParens": "always",
        "trailingComma": "all"
    },
    "files": [
        "lib/**/*",
        "sass/**/*",
        "src/**/*"
    ],
    "jest": {
        "collectCoverageFrom": [
            "<rootDir>/src/**/*.{ts,tsx}",
            "!**/node_modules/**",
            "!**/*.stories.{ts,tsx}"
        ],
        "moduleNameMapper": {
            "\\.(css|scss|sass)$": "identity-obj-proxy"
        },
        "testEnvironment": "jsdom",
        "roots": [
            "<rootDir>/src"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.js"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "dependencies": {
        "date-fns": "^2.29.3",
        "i18next": "^19.8.4",
        "immer": "^9.0.21",
        "react": "^18.2.0",
        "react-date-range": "^1.4.0",
        "react-dom": "^18.2.0",
        "react-focus-lock": "^2.9.4",
        "react-hook-form": "^7.43.8",
        "react-icons": "^4.8.0",
        "use-immer": "^0.8.1"
    },
    "devDependencies": {
        "@babel/core": "^7.21.3",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-private-methods": "^7.18.6",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.21.0",
        "@storybook/addon-actions": "^6.5.15",
        "@storybook/addon-contexts": "^5.3.21",
        "@storybook/addon-essentials": "^6.5.15",
        "@storybook/addon-links": "^6.5.15",
        "@storybook/addon-storysource": "^6.5.15",
        "@storybook/react": "^6.5.15",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/i18next": "^13.0.0",
        "@types/jest": "^29.5.0",
        "@types/react": "^18.0.31",
        "@types/react-date-range": "^1.4.4",
        "@types/react-dom": "^18.0.11",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "core-js": "^3.29.1",
        "cpx": "^1.5.0",
        "eslint": "^8.37.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.3",
        "identity-obj-proxy": "^3.0.0",
        "jest-cli": "^29.3.1",
        "node-glob": "^1.2.0",
        "npm-run-all": "^4.1.5",
        "prettier": "2.8.7",
        "pretty-quick": "^3.1.3",
        "react-hooks-testing-library": "^0.6.0",
        "react-test-renderer": "^18.2.0",
        "rimraf": "^3.0.2",
        "sass": "^1.57.1",
        "sass-loader": "10",
        "storybook": "6.5.16",
        "storybook-addon-pseudo-states": "^1.15.5",
        "ts-key-enum": "^2.0.12",
        "typed-scss-modules": "^3.4.0",
        "typedoc": "^0.22.15",
        "typescript": "^4.6.3"
    },
    "resolutions": {}
}
