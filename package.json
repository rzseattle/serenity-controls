{
  "name": "serenity-controls",
  "version": "2.0.55",
  "description": "",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "watch": "rollup -c -w",
    "dev": "npm-run-all -p storybook 'build:modules -- --watch'",
    "build": "npm-run-all 'build:*'",
    "build:prettier": "prettier --check src",
    "build:rolloup": "rollup -c",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -ci --quiet -c ./docs/stories/.storybook",
    "husky-run": "echo 1",
    "lint": "eslint src/**",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --watchAll=false --coverage",
    "docker:build": "docker build -t serenity-controls .",
    "docker:run": "docker run -v $(pwd):/app -p 6006:6006 serenity-controls",
    "docker:build-run": "npm-run-all docker:build docker:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run build"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prettier": {
    "useTabs": false,
    "tabWidth": 4,
    "printWidth": 120,
    "arrowParens": "always",
    "trailingComma": "all"
  },
  "files": [
    "lib/**/*",
    "sass/**/*",
    "src/**/*"
  ],
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/*.stories.{ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "identity-obj-proxy"
    },
    "testEnvironment": "jest-dom",
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "dnd-core": "^16.0.1",
    "immer": "^9.0.21",
    "react": "^18.2.0",
    "react-date-range": "^1.4.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-focus-lock": "^2.9.6",
    "react-hook-form": "^7.49.2",
    "react-icons": "^4.12.0",
    "ts-key-enum": "^2.0.12",
    "use-immer": "^0.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@storybook/addon-actions": "^7.6.6",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/addon-storysource": "^7.6.6",
    "@storybook/cli": "^7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/react-webpack5": "^7.6.6",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.45",
    "@types/react-date-range": "^1.4.9",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "core-js": "^3.34.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest-cli": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^18.2.0",
    "rollup": "^4.9.1",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "storybook": "^7.6.6",
    "storybook-addon-pseudo-states": "^2.1.2",
    "tslib": "^2.6.2",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@3.6.2"
}
